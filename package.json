{
  "name": "starter-node-react",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=6.10.1 <7.0"
  },
  "scripts": {
    "start": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon --watch server server.js",
    "server-prod": "node server.js",
    "client": "node start-client.js",
    "lint": "eslint .",
    "sequelize": "sequelize",
    "db:seed": "sequelize db:seed:all",
    "db:migrate": "sequelize db:migrate"
  },
  "dependencies": {
    "body-parser": "^1.19.2",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "morgan": "^1.10.0",
    "pg": "^8.7.3",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.17.0",
    "sequelize-cli": "^6.4.1",
    "sequelize-fixtures": "^1.2.0"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "nodemon": "^1.11.0"
  }
}
